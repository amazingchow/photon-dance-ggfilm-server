# uWSGI配置文件
[uwsgi]

# 执行环境
virtualenv = ${project_path}/venv
pythonpath = ${project_path}/venv/lib/python3.6/site-packages

# 项目根目录
chdir = ${project_path}/ggfilm/

# Djnago项目配置
module = django_wsgi
static-map=/static=${project_path}/ggfilm/static

# 工作进程相关配置
master = True
pidfile = ggfilm-server.pid
http = 127.0.0.1:8000
processes = 8
threads = 64
max-requests = 5000
vacuum = True
daemonize = log/uwsgi.log
